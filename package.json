{
  "name": "tillflow",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 6200",
    "build": "cross-env NODE_ENV=production next build",
    "postinstall": "prisma generate --schema=prisma/schema.postgres.prisma",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:wal": "tsx scripts/enable-wal.ts",
    "db:setup": "npm run db:push && npm run db:wal && npm run db:seed",
    "db:reset": "del dev.db 2>nul & npm run db:setup",
    "predev": "prisma generate --schema=prisma/schema.prisma && npm run db:setup"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.6.2",
    "chart.js": "^4.5.1",
    "idb": "^8.0.3",
    "next": "14.2.5",
    "react": "18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^5.1.3",
    "autoprefixer": "^10.4.19",
    "cross-env": "^10.1.0",
    "jsdom": "^28.0.0",
    "postcss": "^8.4.38",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.15.6",
    "typescript": "^5.4.4",
    "vitest": "^4.0.18"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}